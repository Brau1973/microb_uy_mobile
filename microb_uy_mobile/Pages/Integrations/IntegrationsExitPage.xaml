        <StackLayout VerticalOptions="CenterAndExpand" Padding="20">
            <Image Source="dotnet_bot.svg" HeightRequest="150" HorizontalOptions="Center" />
            <Button Text="Salir" Clicked="OnSignOutButtonClicked" Margin="0, 20" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
